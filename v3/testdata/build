exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

exec
CREATE TABLE c (x INT, w INT)
----
table c
  x NULL
  w NULL

build
SELECT y FROM a
----
project [out=(1)]
 ├── columns: a.y:1
 ├── projections
 │    └── variable (a.y) [in=(1) type=int]
 └── inputs
      └── scan [out=(0,1)]
           └── columns: a.x:0 a.y:1

build
SELECT a.* FROM a
----
scan [out=(0,1)]
 └── columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT a.x FROM a)
----
project [out=(0)]
 ├── columns: a.x:0
 ├── projections
 │    └── variable (a.x) [in=(0) type=int]
 └── inputs
      └── scan [out=(0,1)]
           └── columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT a.x+1 FROM a)
----
project [out=(2)]
 ├── columns: .column1:2
 ├── projections
 │    └── plus [in=(0) type=int]
 │         ├── variable (a.x) [in=(0) type=int]
 │         └── const (1) [type=int]
 └── inputs
      └── scan [out=(0,1)]
           └── columns: a.x:0 a.y:1

build
