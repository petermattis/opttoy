exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

exec
CREATE TABLE c (x INT, w INT)
----
table c
  x NULL
  w NULL

build
SELECT y FROM a
----
project [out=(1)]
 |- columns: a.y:1
 |- projections
 |   |- variable (a.y) [in=(1)]
 |- inputs
     |- scan [out=(0,1)]
         |- columns: a.x:0 a.y:1

build
SELECT a.* FROM a
----
scan [out=(0,1)]
 |- columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT a.x FROM a)
----
project [out=(0)]
 |- columns: a.x:0
 |- projections
 |   |- variable (a.x) [in=(0)]
 |- inputs
     |- scan [out=(0,1)]
         |- columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT a.x+1 FROM a)
----
project [out=(2)]
 |- columns: .column1:2
 |- projections
 |   |- plus [in=(0)]
 |       |- inputs
 |           |- variable (a.x) [in=(0)]
 |           |- const (1)
 |- inputs
     |- scan [out=(0,1)]
         |- columns: a.x:0 a.y:1

build
SELECT a.y, a.x FROM a
----
project [out=(0,1)]
 |- columns: a.y:1 a.x:0
 |- projections
 |   |- variable (a.y) [in=(1)]
 |   |- variable (a.x) [in=(0)]
 |- inputs
     |- scan [out=(0,1)]
         |- columns: a.x:0 a.y:1

build
SELECT a.y+1 FROM a
----
project [out=(2)]
 |- columns: .column1:2
 |- projections
 |   |- plus [in=(1)]
 |       |- inputs
 |           |- variable (a.y) [in=(1)]
 |           |- const (1)
 |- inputs
     |- scan [out=(0,1)]
         |- columns: a.x:0 a.y:1

build
SELECT * FROM b WHERE b.z > 10
----
select [out=(0,1)]
 |- columns: b.x:0 b.z:1*
 |- filters
 |   |- gt [in=(1)]
 |       |- inputs
 |           |- variable (b.z) [in=(1)]
 |           |- const (10)
 |- inputs
     |- scan [out=(0,1)]
         |- columns: b.x:0 b.z:1

build
SELECT * FROM a JOIN b USING (x)
----
project [out=(0,1,3)]
 |- columns: a.x:0 a.y:1 b.z:3
 |- equiv: (0,2)
 |- projections
 |   |- variable (a.x) [in=(0)]
 |   |- variable (a.y) [in=(1)]
 |   |- variable (b.z) [in=(3)]
 |- inputs
     |- inner-join [out=(0-3)]
         |- columns: a.x:0* a.y:1 (b.x:2*) b.z:3
         |- equiv: (0,2)
         |- filters
         |   |- eq [in=(0,2)]
         |       |- inputs
         |           |- variable (a.x) [in=(0)]
         |           |- variable (b.x) [in=(2)]
         |- inputs
             |- scan [out=(0,1)]
             |   |- columns: a.x:0 a.y:1
             |- scan [out=(2,3)]
                 |- columns: b.x:2 b.z:3

build
SELECT b.* FROM a JOIN b USING (x)
----
project [out=(3)]
 |- columns: b.z:3
 |- equiv: (0,2)
 |- projections
 |   |- variable (b.z) [in=(3)]
 |- inputs
     |- inner-join [out=(0-3)]
         |- columns: a.x:0* a.y:1 (b.x:2*) b.z:3
         |- equiv: (0,2)
         |- filters
         |   |- eq [in=(0,2)]
         |       |- inputs
         |           |- variable (a.x) [in=(0)]
         |           |- variable (b.x) [in=(2)]
         |- inputs
             |- scan [out=(0,1)]
             |   |- columns: a.x:0 a.y:1
             |- scan [out=(2,3)]
                 |- columns: b.x:2 b.z:3

build
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > 7 AND b.z = 3
----
project [out=(1,3)]
 |- columns: a.y:1 b.z:3
 |- equiv: (0,2)
 |- projections
 |   |- variable (a.y) [in=(1)]
 |   |- variable (b.z) [in=(3)]
 |- inputs
     |- select [out=(0-3)]
         |- columns: a.x:0* a.y:1 b.x:2 b.z:3*
         |- equiv: (0,2)
         |- filters
         |   |- gt [in=(0)]
         |   |   |- inputs
         |   |       |- variable (a.x) [in=(0)]
         |   |       |- const (7)
         |   |- eq [in=(3)]
         |       |- inputs
         |           |- variable (b.z) [in=(3)]
         |           |- const (3)
         |- inputs
             |- inner-join [out=(0-3)]
                 |- columns: a.x:0* a.y:1 b.x:2* b.z:3
                 |- equiv: (0,2)
                 |- filters
                 |   |- eq [in=(0,2)]
                 |       |- inputs
                 |           |- variable (a.x) [in=(0)]
                 |           |- variable (b.x) [in=(2)]
                 |- inputs
                     |- scan [out=(0,1)]
                     |   |- columns: a.x:0 a.y:1
                     |- scan [out=(2,3)]
                         |- columns: b.x:2 b.z:3

build
SELECT b.x FROM a, b WHERE a.x > 7 AND b.z = 3
----
project [out=(2)]
 |- columns: b.x:2
 |- projections
 |   |- variable (b.x) [in=(2)]
 |- inputs
     |- select [out=(0-3)]
         |- columns: a.x:0* a.y:1 b.x:2 b.z:3*
         |- filters
         |   |- gt [in=(0)]
         |   |   |- inputs
         |   |       |- variable (a.x) [in=(0)]
         |   |       |- const (7)
         |   |- eq [in=(3)]
         |       |- inputs
         |           |- variable (b.z) [in=(3)]
         |           |- const (3)
         |- inputs
             |- inner-join [out=(0-3)]
                 |- columns: a.x:0 a.y:1 b.x:2 b.z:3
                 |- inputs
                     |- scan [out=(0,1)]
                     |   |- columns: a.x:0 a.y:1
                     |- scan [out=(2,3)]
                         |- columns: b.x:2 b.z:3

build
