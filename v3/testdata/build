build
SELECT a.x, a.y FROM a UNION SELECT a.y, a.x FROM a
----
union [out=(0,1)]
 ├── columns: a.x:0 a.y:1
 └── inputs
      ├── scan [out=(0,1)]
      │    └── columns: a.x:0 a.y:1
      └── project [out=(2,3)]
           ├── columns: a.y:3 a.x:2
           ├── projections
           │    ├── variable (a.y) [in=(3) type=int]
           │    └── variable (a.x) [in=(2) type=int]
           └── inputs
                └── scan [out=(2,3)]
                     └── columns: a.x:2 a.y:3

build
SELECT b.x FROM a NATURAL JOIN b NATURAL JOIN c
----
project [out=(2)]
 ├── columns: (b.x:2)
 ├── equiv: (0,2,4)
 ├── projections
 │    └── variable (b.x) [in=(2) type=int]
 └── inputs
      └── inner-join [out=(0-5)]
           ├── columns: a.x:0* a.y:1 (b.x:2*) b.z:3 (c.x:4*) c.w:5
           ├── equiv: (0,2,4)
           ├── filters
           │    └── eq [in=(0,4) type=bool]
           │         ├── variable (a.x) [in=(0) type=int]
           │         └── variable (c.x) [in=(4) type=int]
           └── inputs
                ├── inner-join [out=(0-3)]
                │    ├── columns: a.x:0* a.y:1 (b.x:2*) b.z:3
                │    ├── equiv: (0,2)
                │    ├── filters
                │    │    └── eq [in=(0,2) type=bool]
                │    │         ├── variable (a.x) [in=(0) type=int]
                │    │         └── variable (b.x) [in=(2) type=int]
                │    └── inputs
                │         ├── scan [out=(0,1)]
                │         │    └── columns: a.x:0 a.y:1
                │         └── scan [out=(2,3)]
                │              └── columns: b.x:2 b.z:3
                └── scan [out=(4,5)]
                     └── columns: c.x:4 c.w:5
