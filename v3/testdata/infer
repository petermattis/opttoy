CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

exec
CREATE TABLE c (x INT, w INT)
----
table c
  x NULL
  w NULL

build,infer
SELECT * FROM a NATURAL JOIN b NATURAL JOIN c
----
project [out=(0,1,3,5)]
  columns: a.x:0 a.y:1 b.z:3 c.w:5
  equiv: (0,2,4)
  projections:
    variable (a.x) [in=(0)]
    variable (a.y) [in=(1)]
    variable (b.z) [in=(3)]
    variable (c.w) [in=(5)]
  inputs:
    inner join [out=(0,1,2,3,4,5)]
      columns: a.x:0* a.y:1 (b.x:2*) b.z:3 (c.x:4*) c.w:5
      equiv: (0,2,4)
      filters:
        comp (=) [in=(0,4)]
          inputs:
            variable (a.x) [in=(0)]
            variable (c.x) [in=(4)]
        comp (=) [in=(2,4)]
          inputs:
            variable (b.x) [in=(2)]
            variable (c.x) [in=(4)]
        comp (=) [in=(0,2)]
          inputs:
            variable (a.x) [in=(0)]
            variable (b.x) [in=(2)]
        comp (IS NOT) [in=(4)]
          inputs:
            variable (c.x) [in=(4)]
            const (NULL)
      inputs:
        inner join [out=(0,1,2,3)]
          columns: a.x:0* a.y:1 (b.x:2*) b.z:3
          equiv: (0,2)
          filters:
            comp (=) [in=(0,2)]
              inputs:
                variable (a.x) [in=(0)]
                variable (b.x) [in=(2)]
            comp (IS NOT) [in=(0)]
              inputs:
                variable (a.x) [in=(0)]
                const (NULL)
            comp (IS NOT) [in=(2)]
              inputs:
                variable (b.x) [in=(2)]
                const (NULL)
          inputs:
            scan [out=(0,1)]
              columns: a.x:0 a.y:1
            scan [out=(2,3)]
              columns: b.x:2 b.z:3
        scan [out=(4,5)]
          columns: c.x:4 c.w:5

build,infer
SELECT * FROM a, b, c WHERE a.x = b.x AND b.x = c.x
----
inner join [out=(0,1,2,3,4,5)]
  columns: a.x:0* a.y:1 b.x:2* b.z:3 c.x:4* c.w:5
  equiv: (0,2,4)
  filters:
    comp (=) [in=(0,2)]
      inputs:
        variable (a.x) [in=(0)]
        variable (b.x) [in=(2)]
    comp (=) [in=(2,4)]
      inputs:
        variable (b.x) [in=(2)]
        variable (c.x) [in=(4)]
    comp (=) [in=(0,4)]
      inputs:
        variable (a.x) [in=(0)]
        variable (c.x) [in=(4)]
    comp (IS NOT) [in=(0)]
      inputs:
        variable (a.x) [in=(0)]
        const (NULL)
    comp (IS NOT) [in=(2)]
      inputs:
        variable (b.x) [in=(2)]
        const (NULL)
    comp (IS NOT) [in=(4)]
      inputs:
        variable (c.x) [in=(4)]
        const (NULL)
  inputs:
    inner join [out=(0,1,2,3)]
      columns: a.x:0 a.y:1 b.x:2 b.z:3
      inputs:
        scan [out=(0,1)]
          columns: a.x:0 a.y:1
        scan [out=(2,3)]
          columns: b.x:2 b.z:3
    scan [out=(4,5)]
      columns: c.x:4 c.w:5
