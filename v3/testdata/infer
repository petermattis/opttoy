exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

exec
CREATE TABLE c (x INT, w INT)
----
table c
  x NULL
  w NULL

build,infer
SELECT * FROM a NATURAL JOIN b NATURAL JOIN c
----
project [out=(0,1,3,5)]
 ├── columns: a.x:0 a.y:1 b.z:3 c.w:5
 ├── equiv: (0,2,4)
 ├── projections
 │    ├── variable (a.x) [in=(0) type=int]
 │    ├── variable (a.y) [in=(1) type=int]
 │    ├── variable (b.z) [in=(3) type=int]
 │    └── variable (c.w) [in=(5) type=int]
 └── inputs
      └── inner-join [out=(0-5)]
           ├── columns: a.x:0* a.y:1 (b.x:2*) b.z:3 (c.x:4*) c.w:5
           ├── equiv: (0,2,4)
           ├── filters
           │    ├── eq [in=(0,4) type=bool]
           │    │    ├── variable (a.x) [in=(0) type=int]
           │    │    └── variable (c.x) [in=(4) type=int]
           │    ├── eq [in=(2,4) type=bool]
           │    │    ├── variable (b.x) [in=(2) type=int]
           │    │    └── variable (c.x) [in=(4) type=int]
           │    ├── eq [in=(0,2) type=bool]
           │    │    ├── variable (a.x) [in=(0) type=int]
           │    │    └── variable (b.x) [in=(2) type=int]
           │    └── is-not [in=(4) type=bool]
           │         ├── variable (c.x) [in=(4) type=int]
           │         └── const (NULL) [type=NULL]
           └── inputs
                ├── inner-join [out=(0-3)]
                │    ├── columns: a.x:0* a.y:1 (b.x:2*) b.z:3
                │    ├── equiv: (0,2)
                │    ├── filters
                │    │    ├── eq [in=(0,2) type=bool]
                │    │    │    ├── variable (a.x) [in=(0) type=int]
                │    │    │    └── variable (b.x) [in=(2) type=int]
                │    │    ├── is-not [in=(0) type=bool]
                │    │    │    ├── variable (a.x) [in=(0) type=int]
                │    │    │    └── const (NULL) [type=NULL]
                │    │    └── is-not [in=(2) type=bool]
                │    │         ├── variable (b.x) [in=(2) type=int]
                │    │         └── const (NULL) [type=NULL]
                │    └── inputs
                │         ├── scan [out=(0,1)]
                │         │    └── columns: a.x:0 a.y:1
                │         └── scan [out=(2,3)]
                │              └── columns: b.x:2 b.z:3
                └── scan [out=(4,5)]
                     └── columns: c.x:4 c.w:5

build,infer
SELECT * FROM a, b, c WHERE a.x = b.x AND b.x = c.x
----
select [out=(0-5)]
 ├── columns: a.x:0* a.y:1 b.x:2* b.z:3 c.x:4* c.w:5
 ├── equiv: (0,2,4)
 ├── filters
 │    ├── eq [in=(0,2) type=bool]
 │    │    ├── variable (a.x) [in=(0) type=int]
 │    │    └── variable (b.x) [in=(2) type=int]
 │    ├── eq [in=(2,4) type=bool]
 │    │    ├── variable (b.x) [in=(2) type=int]
 │    │    └── variable (c.x) [in=(4) type=int]
 │    ├── eq [in=(0,4) type=bool]
 │    │    ├── variable (a.x) [in=(0) type=int]
 │    │    └── variable (c.x) [in=(4) type=int]
 │    ├── is-not [in=(0) type=bool]
 │    │    ├── variable (a.x) [in=(0) type=int]
 │    │    └── const (NULL) [type=NULL]
 │    ├── is-not [in=(2) type=bool]
 │    │    ├── variable (b.x) [in=(2) type=int]
 │    │    └── const (NULL) [type=NULL]
 │    └── is-not [in=(4) type=bool]
 │         ├── variable (c.x) [in=(4) type=int]
 │         └── const (NULL) [type=NULL]
 └── inputs
      └── inner-join [out=(0-5)]
           ├── columns: a.x:0 a.y:1 b.x:2 b.z:3 c.x:4 c.w:5
           └── inputs
                ├── inner-join [out=(0-3)]
                │    ├── columns: a.x:0 a.y:1 b.x:2 b.z:3
                │    └── inputs
                │         ├── scan [out=(0,1)]
                │         │    └── columns: a.x:0 a.y:1
                │         └── scan [out=(2,3)]
                │              └── columns: b.x:2 b.z:3
                └── scan [out=(4,5)]
                     └── columns: c.x:4 c.w:5
