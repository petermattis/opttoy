exec
CREATE TABLE d (
  dept_id INT PRIMARY KEY,
  name STRING
)
----
table d
  dept_id NOT NULL
  name NULL
  (dept_id) KEY

exec
CREATE TABLE e (
  emp_id INT PRIMARY KEY,
  dept_id INT REFERENCES d (dept_id),
  name STRING,
  salary INT
)
----
table e
  emp_id NOT NULL
  dept_id NULL
  name NULL
  salary NULL
  (emp_id) KEY
  (dept_id) -> d(dept_id)

prep
SELECT e.name, e.salary
FROM e, d
WHERE e.dept_id = d.dept_id
----
project [out=(2,3)]
 ├── columns: e.name:2 e.salary:3
 ├── equiv: (1,4)
 ├── projections
 │    ├── variable (e."name") [in=(2) type=string]
 │    └── variable (e.salary) [in=(3) type=int]
 └── inputs
      └── select [out=(1-3)]
           ├── columns: e.emp_id:0 e.dept_id:1* e.name:2 e.salary:3
           ├── weak key: (0)
           ├── foreign key: (1) -> (4)
           ├── filters
           │    └── IS-NOT [in=(1) type=bool]
           │         └── inputs
           │              ├── variable (e.dept_id) [in=(1) type=int]
           │              └── const (NULL) [type=NULL]
           └── inputs
                └── scan [out=(1-3)]
                     ├── columns: e.emp_id:0* e.dept_id:1 e.name:2 e.salary:3
                     ├── key: (0)
                     └── foreign key: (1) -> (4)

prep
SELECT e.name, e.salary
FROM d, e
WHERE e.dept_id = d.dept_id AND e.dept_id != 0
----
project [out=(4,5)]
 ├── columns: e.name:4 e.salary:5
 ├── equiv: (0,3)
 ├── projections
 │    ├── variable (e."name") [in=(4) type=string]
 │    └── variable (e.salary) [in=(5) type=int]
 └── inputs
      └── select [out=(3-5)]
           ├── columns: e.emp_id:2 e.dept_id:3* e.name:4 e.salary:5
           ├── weak key: (2)
           ├── foreign key: (3) -> (0)
           ├── filters
           │    └── ne [in=(3) type=bool]
           │         └── inputs
           │              ├── variable (e.dept_id) [in=(3) type=int]
           │              └── const (0) [type=int]
           └── inputs
                └── scan [out=(3-5)]
                     ├── columns: e.emp_id:2* e.dept_id:3 e.name:4 e.salary:5
                     ├── key: (2)
                     └── foreign key: (3) -> (0)

prep
SELECT e.name, e.salary, d.name
FROM e, d
WHERE e.dept_id = d.dept_id
----
project [out=(2,3,5)]
 ├── columns: e.name:2 e.salary:3 d.name:5
 ├── equiv: (1,4)
 ├── projections
 │    ├── variable (e."name") [in=(2) type=string]
 │    ├── variable (e.salary) [in=(3) type=int]
 │    └── variable (d."name") [in=(5) type=string]
 └── inputs
      └── inner-join [out=(2,3,5)]
           ├── columns: e.emp_id:0 e.dept_id:1* e.name:2 e.salary:3 d.dept_id:4* d.name:5
           ├── equiv: (1,4)
           ├── filters
           │    └── eq [in=(1,4) type=bool]
           │         └── inputs
           │              ├── variable (e.dept_id) [in=(1) type=int]
           │              └── variable (d.dept_id) [in=(4) type=int]
           └── inputs
                ├── select [out=(1-3)]
                │    ├── columns: e.emp_id:0 e.dept_id:1* e.name:2 e.salary:3
                │    ├── weak key: (0)
                │    ├── foreign key: (1) -> (4)
                │    ├── filters
                │    │    └── IS-NOT [in=(1) type=bool]
                │    │         └── inputs
                │    │              ├── variable (e.dept_id) [in=(1) type=int]
                │    │              └── const (NULL) [type=NULL]
                │    └── inputs
                │         └── scan [out=(1-3)]
                │              ├── columns: e.emp_id:0* e.dept_id:1 e.name:2 e.salary:3
                │              ├── key: (0)
                │              └── foreign key: (1) -> (4)
                └── scan [out=(4,5)]
                     ├── columns: d.dept_id:4* d.name:5
                     └── key: (4)
