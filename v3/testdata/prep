exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

prep
SELECT a.x FROM a
----
project [out=(0)]
 ├── columns: a.x:0
 ├── projections
 │    └── variable (a.x) [in=(0) type=int]
 └── inputs
      └── scan [out=(0)]
           └── columns: a.x:0 a.y:1

prep
SELECT x FROM a WHERE y > 1
----
project [out=(0)]
 ├── columns: a.x:0
 ├── projections
 │    └── variable (a.x) [in=(0) type=int]
 └── inputs
      └── select [out=(0)]
           ├── columns: a.x:0 a.y:1*
           ├── filters
           │    └── gt [in=(1) type=bool]
           │         ├── variable (a.y) [in=(1) type=int]
           │         └── const (1) [type=int]
           └── inputs
                └── scan [out=(0,1)]
                     └── columns: a.x:0 a.y:1

prep
SELECT * FROM a NATURAL JOIN b WHERE a.x > 1
----
project [out=(0,1,3)]
 ├── columns: a.x:0 a.y:1 b.z:3
 ├── equiv: (0,2)
 ├── projections
 │    ├── variable (a.x) [in=(0) type=int]
 │    ├── variable (a.y) [in=(1) type=int]
 │    └── variable (b.z) [in=(3) type=int]
 └── inputs
      └── inner-join [out=(0,1,3)]
           ├── columns: a.x:0* a.y:1 (b.x:2*) b.z:3
           ├── equiv: (0,2)
           ├── filters
           │    └── eq [in=(0,2) type=bool]
           │         ├── variable (a.x) [in=(0) type=int]
           │         └── variable (b.x) [in=(2) type=int]
           └── inputs
                ├── select [out=(0,1)]
                │    ├── columns: a.x:0* a.y:1
                │    ├── filters
                │    │    ├── gt [in=(0) type=bool]
                │    │    │    ├── variable (a.x) [in=(0) type=int]
                │    │    │    └── const (1) [type=int]
                │    │    └── is-not [in=(0) type=bool]
                │    │         ├── variable (a.x) [in=(0) type=int]
                │    │         └── const (NULL) [type=NULL]
                │    └── inputs
                │         └── scan [out=(0,1)]
                │              └── columns: a.x:0 a.y:1
                └── select [out=(2,3)]
                     ├── columns: b.x:2* b.z:3
                     ├── filters
                     │    ├── gt [in=(2) type=bool]
                     │    │    ├── variable (b.x) [in=(2) type=int]
                     │    │    └── const (1) [type=int]
                     │    └── is-not [in=(2) type=bool]
                     │         ├── variable (b.x) [in=(2) type=int]
                     │         └── const (NULL) [type=NULL]
                     └── inputs
                          └── scan [out=(2,3)]
                               └── columns: b.x:2 b.z:3
