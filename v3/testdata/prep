exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

prep
SELECT a.x FROM a
----
project [out=(0)]
  columns: a.x:0
  projections:
    variable (a.x) [in=(0)]
  inputs:
    scan [out=(0)]
      columns: a.x:0 a.y:1

prep
SELECT x FROM a WHERE y > 1
----
project [out=(0)] [outer=(1)]
  columns: a.x:0
  projections:
    variable (a.x) [in=(0)]
  inputs:
    scan [out=(0)] [outer=(1)]
      columns: a.x:0 a.y:1*
      filters:
        comp (>) [in=(1)]
          inputs:
            variable (a.y) [in=(1)]
            const (1)

prep
SELECT * FROM a NATURAL JOIN b WHERE a.x > 1
----
project [out=(0,1,3)]
  columns: a.x:0 a.y:1 b.z:3
  equiv: (0,2)
  projections:
    variable (a.x) [in=(0)]
    variable (a.y) [in=(1)]
    variable (b.z) [in=(3)]
  inputs:
    inner join [out=(0,1,3)]
      columns: a.x:0* a.y:1 (b.x:2*) b.z:3
      equiv: (0,2)
      filters:
        comp (=) [in=(0,2)]
          inputs:
            variable (a.x) [in=(0)]
            variable (b.x) [in=(2)]
      inputs:
        scan [out=(0,1)]
          columns: a.x:0* a.y:1
          filters:
            comp (>) [in=(0)]
              inputs:
                variable (a.x) [in=(0)]
                const (1)
        scan [out=(2,3)]
          columns: b.x:2* b.z:3
          filters:
            comp (>) [in=(2)]
              inputs:
                variable (b.x) [in=(2)]
                const (1)
