CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

exec
CREATE TABLE c (x INT, w INT)
----
table c
  x NULL
  w NULL

prep
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > $1 AND b.z = $2
----
project [out=(1,3)]
 |- columns: a.y:1 b.z:3
 |- equiv: (0,2)
 |- projections
 |   |- variable (a.y) [in=(1)]
 |   |- variable (b.z) [in=(3)]
 |- inputs
     |- inner-join [out=(1,3)]
         |- columns: a.x:0* a.y:1 b.x:2* b.z:3*
         |- equiv: (0,2)
         |- filters
         |   |- eq [in=(0,2)]
         |       |- inputs
         |           |- variable (a.x) [in=(0)]
         |           |- variable (b.x) [in=(2)]
         |- inputs
             |- select [out=(0,1)]
             |   |- columns: a.x:0* a.y:1
             |   |- filters
             |   |   |- gt [in=(0)]
             |   |   |   |- inputs
             |   |   |       |- variable (a.x) [in=(0)]
             |   |   |       |- placeholder ($1)
             |   |   |- IS-NOT [in=(0)]
             |   |       |- inputs
             |   |           |- variable (a.x) [in=(0)]
             |   |           |- const (NULL)
             |   |- inputs
             |       |- scan [out=(0,1)]
             |           |- columns: a.x:0 a.y:1
             |- select [out=(2,3)]
                 |- columns: b.x:2* b.z:3*
                 |- filters
                 |   |- eq [in=(3)]
                 |   |   |- inputs
                 |   |       |- variable (b.z) [in=(3)]
                 |   |       |- placeholder ($2)
                 |   |- gt [in=(2)]
                 |   |   |- inputs
                 |   |       |- variable (b.x) [in=(2)]
                 |   |       |- placeholder ($1)
                 |   |- IS-NOT [in=(2)]
                 |       |- inputs
                 |           |- variable (b.x) [in=(2)]
                 |           |- const (NULL)
                 |- inputs
                     |- scan [out=(2,3)]
                         |- columns: b.x:2 b.z:3

prep
SELECT * FROM a JOIN b ON (a.x = b.x) WHERE a.y + b.z > 1
----
inner-join [out=(0-3)]
 |- columns: a.x:0* a.y:1* b.x:2* b.z:3*
 |- equiv: (0,2)
 |- filters
 |   |- eq [in=(0,2)]
 |   |   |- inputs
 |   |       |- variable (a.x) [in=(0)]
 |   |       |- variable (b.x) [in=(2)]
 |   |- gt [in=(1,3)]
 |       |- inputs
 |           |- plus [in=(1,3)]
 |           |   |- inputs
 |           |       |- variable (a.y) [in=(1)]
 |           |       |- variable (b.z) [in=(3)]
 |           |- const (1)
 |- inputs
     |- select [out=(0,1)]
     |   |- columns: a.x:0* a.y:1*
     |   |- filters
     |   |   |- IS-NOT [in=(1)]
     |   |   |   |- inputs
     |   |   |       |- variable (a.y) [in=(1)]
     |   |   |       |- const (NULL)
     |   |   |- IS-NOT [in=(0)]
     |   |       |- inputs
     |   |           |- variable (a.x) [in=(0)]
     |   |           |- const (NULL)
     |   |- inputs
     |       |- scan [out=(0,1)]
     |           |- columns: a.x:0 a.y:1
     |- select [out=(2,3)]
         |- columns: b.x:2* b.z:3*
         |- filters
         |   |- IS-NOT [in=(3)]
         |   |   |- inputs
         |   |       |- variable (b.z) [in=(3)]
         |   |       |- const (NULL)
         |   |- IS-NOT [in=(2)]
         |       |- inputs
         |           |- variable (b.x) [in=(2)]
         |           |- const (NULL)
         |- inputs
             |- scan [out=(2,3)]
                 |- columns: b.x:2 b.z:3

prep
SELECT * FROM (SELECT a.x+1 FROM a) AS b(x) WHERE x > $1
----
project [out=(2)]
 |- columns: b.x:2
 |- projections
 |   |- plus [in=(0)]
 |       |- inputs
 |           |- variable (a.x) [in=(0)]
 |           |- const (1)
 |- inputs
     |- select [out=(0)]
         |- columns: a.x:0* a.y:1
         |- filters
         |   |- gt [in=(0)]
         |       |- inputs
         |           |- plus [in=(0)]
         |           |   |- inputs
         |           |       |- variable (a.x) [in=(0)]
         |           |       |- const (1)
         |           |- placeholder ($1)
         |- inputs
             |- scan [out=(0)]
                 |- columns: a.x:0 a.y:1

prep
SELECT * FROM a, b, c WHERE a.x = b.x AND b.x = c.x
----
inner-join [out=(0-5)]
 |- columns: a.x:0* a.y:1 b.x:2* b.z:3 c.x:4* c.w:5
 |- equiv: (0,2,4)
 |- filters
 |   |- eq [in=(2,4)]
 |   |   |- inputs
 |   |       |- variable (b.x) [in=(2)]
 |   |       |- variable (c.x) [in=(4)]
 |   |- eq [in=(0,4)]
 |       |- inputs
 |           |- variable (a.x) [in=(0)]
 |           |- variable (c.x) [in=(4)]
 |- inputs
     |- inner-join [out=(0-3)]
     |   |- columns: a.x:0* a.y:1 b.x:2* b.z:3
     |   |- equiv: (0,2)
     |   |- filters
     |   |   |- eq [in=(0,2)]
     |   |       |- inputs
     |   |           |- variable (a.x) [in=(0)]
     |   |           |- variable (b.x) [in=(2)]
     |   |- inputs
     |       |- select [out=(0,1)]
     |       |   |- columns: a.x:0* a.y:1
     |       |   |- filters
     |       |   |   |- IS-NOT [in=(0)]
     |       |   |       |- inputs
     |       |   |           |- variable (a.x) [in=(0)]
     |       |   |           |- const (NULL)
     |       |   |- inputs
     |       |       |- scan [out=(0,1)]
     |       |           |- columns: a.x:0 a.y:1
     |       |- select [out=(2,3)]
     |           |- columns: b.x:2* b.z:3
     |           |- filters
     |           |   |- IS-NOT [in=(2)]
     |           |       |- inputs
     |           |           |- variable (b.x) [in=(2)]
     |           |           |- const (NULL)
     |           |- inputs
     |               |- scan [out=(2,3)]
     |                   |- columns: b.x:2 b.z:3
     |- select [out=(4,5)]
         |- columns: c.x:4* c.w:5
         |- filters
         |   |- IS-NOT [in=(4)]
         |       |- inputs
         |           |- variable (c.x) [in=(4)]
         |           |- const (NULL)
         |- inputs
             |- scan [out=(4,5)]
                 |- columns: c.x:4 c.w:5

prep
SELECT * FROM a AS l JOIN a AS r ON (l.x = r.y)
----
inner-join [out=(0-3)]
 |- columns: l.x:0* l.y:1 r.x:2 r.y:3*
 |- equiv: (0,3)
 |- filters
 |   |- eq [in=(0,3)]
 |       |- inputs
 |           |- variable (l.x) [in=(0)]
 |           |- variable (r.y) [in=(3)]
 |- inputs
     |- select [out=(0,1)]
     |   |- columns: l.x:0* l.y:1
     |   |- filters
     |   |   |- IS-NOT [in=(0)]
     |   |       |- inputs
     |   |           |- variable (l.x) [in=(0)]
     |   |           |- const (NULL)
     |   |- inputs
     |       |- scan [out=(0,1)]
     |           |- columns: l.x:0 l.y:1
     |- select [out=(2,3)]
         |- columns: r.x:2 r.y:3*
         |- filters
         |   |- IS-NOT [in=(3)]
         |       |- inputs
         |           |- variable (r.y) [in=(3)]
         |           |- const (NULL)
         |- inputs
             |- scan [out=(2,3)]
                 |- columns: r.x:2 r.y:3
