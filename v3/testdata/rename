exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

build
SELECT * FROM (SELECT * FROM a) AS b
----
scan [out=(0,1)]
 └── columns: b.x:0 b.y:1

build
SELECT * FROM (SELECT * FROM a) AS b (e, f)
----
scan [out=(0,1)]
 └── columns: b.e:0 b.f:1

build
SELECT d.*, c.* FROM a AS c NATURAL JOIN b AS d
----
project [out=(0,1,3)]
 ├── columns: d.z:3 c.x:0 c.y:1
 ├── equiv: (0,2)
 ├── projections
 │    ├── variable (d.z) [in=(3) type=int]
 │    ├── variable (c.x) [in=(0) type=int]
 │    └── variable (c.y) [in=(1) type=int]
 └── inputs
      └── inner-join [out=(0-3)]
           ├── columns: c.x:0* c.y:1 (d.x:2*) d.z:3
           ├── equiv: (0,2)
           ├── filters
           │    └── eq [in=(0,2) type=bool]
           │         ├── variable (c.x) [in=(0) type=int]
           │         └── variable (d.x) [in=(2) type=int]
           └── inputs
                ├── scan [out=(0,1)]
                │    └── columns: c.x:0 c.y:1
                └── scan [out=(2,3)]
                     └── columns: d.x:2 d.z:3

build
SELECT x AS e FROM a
----
project [out=(0)]
 ├── columns: a.e:0
 ├── projections
 │    └── variable (a.x) [in=(0) type=int]
 └── inputs
      └── scan [out=(0,1)]
           └── columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT x AS e FROM a)
----
project [out=(0)]
 ├── columns: a.e:0
 ├── projections
 │    └── variable (a.x) [in=(0) type=int]
 └── inputs
      └── scan [out=(0,1)]
           └── columns: a.x:0 a.y:1

prep,memo
SELECT * FROM a AS a1 JOIN b AS b1 ON (a1.x = b1.x)
----
11: [inner-join [5 9 10]]
10: [eq [2 7]]
9: [select [6 8]]
8: [is-not [7 3]]
7: [variable b1.x]
6: [scan b]
5: [select [1 4]]
4: [is-not [2 3]]
3: [const NULL]
2: [variable a1.x]
1: [scan a]
