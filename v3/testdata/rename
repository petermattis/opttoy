exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

build
SELECT * FROM (SELECT * FROM a) AS b
----
rename [out=(0,1)]
  columns: b.x:0 b.y:1
  inputs:
    scan [out=(0,1)]
      columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT * FROM a) AS b (e, f)
----
rename [out=(0,1)]
  columns: b.e:0 b.f:1
  inputs:
    scan [out=(0,1)]
      columns: a.x:0 a.y:1

build
SELECT d.*, c.* FROM a AS c NATURAL JOIN b AS d
----
project [out=(0,1,3)]
  columns: d.z:3 c.x:0 c.y:1
  equiv: (0,2)
  projections:
    variable (d.z) [in=(3)]
    variable (c.x) [in=(0)]
    variable (c.y) [in=(1)]
  inputs:
    inner join [out=(0,1,2,3)]
      columns: c.x:0* c.y:1 (d.x:2*) d.z:3
      equiv: (0,2)
      filters:
        comp (=) [in=(0,2)]
          inputs:
            variable (c.x) [in=(0)]
            variable (d.x) [in=(2)]
      inputs:
        rename [out=(0,1)]
          columns: c.x:0 c.y:1
          inputs:
            scan [out=(0,1)]
              columns: a.x:0 a.y:1
        rename [out=(2,3)]
          columns: d.x:2 d.z:3
          inputs:
            scan [out=(2,3)]
              columns: b.x:2 b.z:3

build
SELECT x AS e FROM a
----
project [out=(0)]
  columns: a.e:0
  projections:
    variable (a.x) [in=(0)]
  inputs:
    scan [out=(0,1)]
      columns: a.x:0 a.y:1

build
SELECT * FROM (SELECT x AS e FROM a)
----
project [out=(0)]
  columns: a.e:0
  projections:
    variable (a.x) [in=(0)]
  inputs:
    scan [out=(0,1)]
      columns: a.x:0 a.y:1
