exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
INSERT INTO histogram.a.x VALUES
  ('rows', 1000),
  ('distinct', 100),
  ('nulls', 10),
  ('lowerBound', 0),
  (4, 5, 2), (2, 3, 3), (100, 976, 1)
----
rows:       1000
distinct:   100
nulls:      10
lowerBound: 0
buckets:  2:3,3 4:5,2 100:976,1

exec
