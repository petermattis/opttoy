----
project
 ├── columns: x:3
 ├── select
 │    ├── columns: a.x:1* a.y:2 b.x:3 b.z:4*
 │    ├── inner-join
 │    │    ├── columns: a.x:1 a.y:2 b.x:3 b.z:4
 │    │    ├── scan
 │    │    │    └── columns: a.x:1 a.y:2
 │    │    ├── scan
 │    │    │    └── columns: b.x:3 b.z:4
 │    │    └── true
 │    └── and [unbound=(1,4)]
 │         ├── gt [unbound=(1)]
 │         │    ├── variable: a.x [unbound=(1)]
 │         │    └── const: 7
 │         └── eq [unbound=(4)]
 │              ├── variable: b.z [unbound=(4)]
 │              └── const: 3
 └── projections [unbound=(3)]
      └── variable: b.x [unbound=(3)]

