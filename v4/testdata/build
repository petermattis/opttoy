 ├── columns: x:1 z:2*
 └── select
      ├── columns: b.x:1 b.z:2*
      ├── scan
      │    └── columns: b.x:1 b.z:2
      └── gt [unbound=(2)]
           ├── variable: b.z [unbound=(2)]
           └── const: 10

build
SELECT * FROM a JOIN b USING (x)
----
project
 ├── columns: x:1* y:2 z:4
 ├── equiv: (1,3)
 ├── inner-join
 │    ├── columns: a.x:1* a.y:2 b.x:3* b.z:4
 │    ├── equiv: (1,3)
 │    ├── scan
 │    │    └── columns: a.x:1 a.y:2
 │    ├── scan
 │    │    └── columns: b.x:3 b.z:4
 │    └── filter-list [unbound=(1,3)]
 │         └── eq [unbound=(1,3)]
 │              ├── variable: a.x [unbound=(1)]
 │              └── variable: b.x [unbound=(3)]
 └── projections [unbound=(1,2,4)]
      ├── variable: a.x [unbound=(1)]
      ├── variable: a.y [unbound=(2)]
      └── variable: b.z [unbound=(4)]

build
SELECT b.* FROM a JOIN b USING (x)
----
project
 ├── columns: z:4
 ├── equiv: (1,3)
 ├── inner-join
 │    ├── columns: a.x:1* a.y:2 b.x:3* b.z:4
 │    ├── equiv: (1,3)
 │    ├── scan
 │    │    └── columns: a.x:1 a.y:2
 │    ├── scan
 │    │    └── columns: b.x:3 b.z:4
 │    └── filter-list [unbound=(1,3)]
 │         └── eq [unbound=(1,3)]
 │              ├── variable: a.x [unbound=(1)]
 │              └── variable: b.x [unbound=(3)]
 └── projections [unbound=(4)]
      └── variable: b.z [unbound=(4)]

build
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > 7 AND b.z = 3
----
project
 ├── columns: y:2 z:4*
 ├── equiv: (1,3)
 ├── select
 │    ├── columns: a.x:1* a.y:2 b.x:3* b.z:4*
 │    ├── equiv: (1,3)
 │    ├── inner-join
 │    │    ├── columns: a.x:1* a.y:2 b.x:3* b.z:4
 │    │    ├── equiv: (1,3)
 │    │    ├── scan
 │    │    │    └── columns: a.x:1 a.y:2
 │    │    ├── scan
 │    │    │    └── columns: b.x:3 b.z:4
 │    │    └── eq [unbound=(1,3)]
 │    │         ├── variable: a.x [unbound=(1)]
 │    │         └── variable: b.x [unbound=(3)]
 │    └── and [unbound=(1,4)]
 │         ├── gt [unbound=(1)]
 │         │    ├── variable: a.x [unbound=(1)]
 │         │    └── const: 7
 │         └── eq [unbound=(4)]
 │              ├── variable: b.z [unbound=(4)]
 │              └── const: 3
 └── projections [unbound=(2,4)]
      ├── variable: a.y [unbound=(2)]
      └── variable: b.z [unbound=(4)]

build
