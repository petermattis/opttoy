exec
CREATE TABLE a (x INT, y INT)
----
table a
  x NULL
  y NULL

exec
CREATE TABLE b (x INT, z INT)
----
table b
  x NULL
  z NULL

exec
CREATE TABLE c (x INT, w INT)
----
table c
  x NULL
  w NULL

normalize
SELECT a.y, b.z FROM a JOIN b ON (a.x = b.x) WHERE a.x > $1 AND b.z = $2
----
project
 ├── columns: y:2 z:4*
 ├── equiv: (1,3)
 ├── inner-join
 │    ├── columns: a.x:1* a.y:2 b.x:3* b.z:4*
 │    ├── equiv: (1,3)
 │    ├── select
 │    │    ├── columns: a.x:1* a.y:2
 │    │    ├── scan
 │    │    │    └── columns: a.x:1 a.y:2
 │    │    └── filters [unbound=(1)]
 │    │         └── gt [unbound=(1)]
 │    │              ├── variable: a.x [unbound=(1)]
 │    │              └── placeholder: $1
 │    ├── select
 │    │    ├── columns: b.x:3* b.z:4*
 │    │    ├── scan
 │    │    │    └── columns: b.x:3 b.z:4
 │    │    └── filters [unbound=(3,4)]
 │    │         ├── eq [unbound=(4)]
 │    │         │    ├── variable: b.z [unbound=(4)]
 │    │         │    └── placeholder: $2
 │    │         └── gt [unbound=(3)]
 │    │              ├── variable: b.x [unbound=(3)]
 │    │              └── placeholder: $1
 │    └── filters [unbound=(1,3)]
 │         └── eq [unbound=(1,3)]
 │              ├── variable: a.x [unbound=(1)]
 │              └── variable: b.x [unbound=(3)]
 └── projections [unbound=(2,4)]
      ├── variable: a.y [unbound=(2)]
      └── variable: b.z [unbound=(4)]
